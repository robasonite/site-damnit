<div id="page-header">
  <h1>{{page_title}}</h1>
  <div class="page-meta">
    {{#page_has_datetime}}
    <time datetime="{{page_datetime}}">{{page_datetime}}</time>
    {{/page_has_datetime}}
    {{#page_has_tags}}
    <ul>
      {{#page_tags}}
      <li>{{name}}</li>
      {{/page_tags}}
    </ul>
    {{/page_has_tags}}
  </div>
</div>
<div id="page-content">
  {{{page_content}}}
</div>
